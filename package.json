{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "description": "Express, Typescript, Inversify, Postgres",
  "author": "Leo Claudio",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test && node --max-old-space-size=4096 ./node_modules/.bin/jest --runInBand --detectOpenHandles --force-exit",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "prettier": "prettier \"**/*.{js,ts}\"",
    "prettier:check": "prettier --check \"**/*.{js,ts}\"",
    "format": "npm run prettier -- --write",
    "lint": "eslint \"**/*.{js,ts}\" ",
    "lint:fix": "npm run lint -- --fix",
    "validate": "npm run prettier -- --list-different && npm run lint",
    "cm": "./node_modules/.bin/sequelize migration:generate --name",
    "cs": "./node_modules/.bin/sequelize seed:generate --name"
  },
  "dependencies": {
    "aws-sdk": "^2.670.0",
    "axios": "^0.21.1",
    "config": "^3.1.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "inversify": "^5.0.1",
    "joi": "^14.3.1",
    "pg": "^7.11.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.3",
    "speakeasy": "^2.0.0"
  },
  "devDependencies": {
    "@types/speakeasy": "^2.0.5",
    "@types/aws-lambda": "^8.10.31",
    "@types/config": "0.0.34",
    "@types/jest": "^24.0.12",
    "@types/joi": "^14.3.3",
    "@types/sequelize": "^4.27.49",
    "@types/umzug": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "sequelize-cli": "^5.5.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^8.1.0",
    "typescript": "^3.7.5",
    "umzug": "^2.2.0"
  }
}
